<gexf xmlns="http://www.gexf.net/1.2draft" xmlns:viz="http://www.gexf.net/1.2draft/viz" version="1.2">
    <graph mode="static" defaultedgetype="undirected">
        <nodes>
            <node id="0" label="Hello" />
            <node id="1" label="World!" />
            <node id="2" label="Viz Test!">
                <viz:color r="0" g="255" b="0" />
                <viz:shape value="diamond" />
                <viz:size value="3.4" />
            </node>
        </nodes>
        <edges>
            <edge id="0" source="0" target="1" />
            <edge id="1" source="1" target="0" type="directed">
                <viz:shape value="dotted" />
                <viz:thickness value="2.4" />
                <viz:color r="255" b="255" g="0" />
            </edge>
        </edges>
    </graph>
</gexf>